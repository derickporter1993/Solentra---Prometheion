        <label>Flow Name</label>
        <type>Text</type>
        <length>255</length>
    </fields>
    <fields>
        <fullName>Primary_Record__c</fullName>
        <label>Primary Record</label>
        <referenceTo>Account</referenceTo>
        <relationshipLabel>Primary Records</relationshipLabel>
        <relationshipName>Primary_Records</relationshipName>
        <type>Lookup</type>
    </fields>
    <fields>
        <fullName>Status__c</fullName>
        <label>Status</label>
        <type>Picklist</type>
